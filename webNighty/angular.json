              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "android:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "android:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "android:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/android/tsconfig.app.json",
              "apps/android/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/android/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/android/jest.config.js",
            "tsConfig": "apps/android/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/android/src/test-setup.ts"
          }
        },
        "ionic-cordova-serve": {
          "builder": "@ionic/angular-toolkit:cordova-serve",
          "options": {
            "cordovaBuildTarget": "android:ionic-cordova-build",
            "devServerTarget": "android:serve"
          },
          "configurations": {
            "production": {
              "cordovaBuildTarget": "android:ionic-cordova-build:production",
              "devServerTarget": "android:serve:production"
            }
          }
        },
        "ionic-cordova-build": {
          "builder": "@ionic/angular-toolkit:cordova-build",
          "options": {
            "browserTarget": "android:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "android:build:production"
            }
          }
        }
      }
